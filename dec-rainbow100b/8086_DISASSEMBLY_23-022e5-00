seg000:3E3B                 push    ds
seg000:3E3C                 push    es
seg000:3E3D                 pop     ds
seg000:3E3E                 sub     bx, bx
seg000:3E40                 mov     bp, 0FCh ; 'n'
seg000:3E43                 call    j_CRC_CHECK1__loc_2A3 ; CRC check(s)

seg000:1E8E                 mov     [bx+2], ax
seg000:1E91                 mov     bx, 8Ch ; 'î'
seg000:1E94                 mov     word ptr [bx], 1EBCh ; [OPTIONAL COMM. BOARD]   ( mem.location $8c for non-DMA access )
seg000:1E98                 mov     [bx+2], ax
